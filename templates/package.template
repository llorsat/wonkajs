{
  "name": "%(name)s",
  "description": "Write a little bit about this project",
  "version": "0.0.1",
  "author": "",
  "homepage": "",
  "_id": "%(name)s@0.0.1",
  "settings": {
    "i18n": {
      "languages": [
        "es",
        "en"
      ],
      "files": [
        "js",
        "html"
      ],
      "blacklist": [
        ".git",
        "images",
        "icons",
        "stylesheets",
        "libraries"
      ]
    },
    "deploy": {
      "js": {
        "src": ["%(jsSrcDeployPath)s"],
        "dest": "%(jsDestDeployPath)s"
      }
    },
    "environment": {
      "core": [
        "app",
        "run",
        "contrib/modernizr"
      ],
      "libraries": [
        "jquery",
        "jquery.easing",
        "jquery.ui",
        "spin",
        "bootstrap",
        "handlebars",
        "underscore",
        "backbone",
        "globalize",
        "jquery.vitrine",
        "jquery.vitrine.helpers",
        "backbone.extensions",
        "handlebars.helpers",
        "console",
        "helpers"
      ],
      "main_files": [
        "listeners",
        "alerts",
        "main"
      ],
      "applications": [
        "root"
      ]
    }
  }
}