    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <link rel="shortcut icon" href="/icons/favicon.ico">
        <link rel="apple-touch-icon" sizes="144x144" href="/icons/touch-icon-144x144.png" />
        <link rel="apple-touch-icon" sizes="114x114" href="/icons/touch-icon-114x114.png" />
        <link rel="apple-touch-icon" sizes="72x72" href="/icons/touch-icon-72x72.png" />
        <link rel="apple-touch-icon" sizes="57x57" href="/icons/touch-icon-57x57.png" />
        <link rel="apple-touch-icon" href="/icons/touch-icon-iphone.png" />
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        <!-- edit under your own risk -->
        {{# if development }}
          <link rel="stylesheet/less" href="stylesheets/ui.less">
          <script src="core/contrib/less.js"></script>
          <script src="core/contrib/modernizr.js"></script>
        {{ else }}
          <link type="text/css" rel="stylesheet" href="main.css?{{ version }}">
        {{/ if }}
        <!-- /edit under your own risk -->
    </head>
    <body>
        <!--[if lt IE 7]>
            <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
        <![endif]-->
        <div id="install-bar">
          <button id="install-button">Install</button>
        </div>
        <div id="container"></div>

        <!-- edit under your own risk -->
        {{# each templates }}
          {{{ this }}}
        {{/ each }}

        {{# if development }}
          {{# each scripts }}
          <script type="text/javascript" src="{{ this }}"></script>
          {{/ each }}
          <script type="text/javascript">(function(){window.main();})();</script>
        {{ else }}
          <script type="text/javascript" src="main.js?{{ version }}"></script>
        {{/ if }}
        <!-- /edit under your own risk -->
    </body>
</html>
